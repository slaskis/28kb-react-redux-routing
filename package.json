{
  "name": "28kb-react-redux-routing",
  "description": "your awesome stack doesnt have to be fat",
  "private": true,
  "dependencies": {
    "aphrodite": "^0.2.0",
    "enroute": "^1.0.1",
    "koa": "^2.0.0-alpha.3",
    "koa-convert": "^1.2.0",
    "koa-static": "^2.0.0",
    "preact-render-to-string": "^2.1.0",
    "preact-socrates": "matthewmueller/preact-socrates#1.0.1",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-routes": "^1.0.0",
    "reselect": "^2.2.1",
    "socrates": "^1.0.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-async-to-generator": "^6.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-jsx": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "nodemon": "^1.9.1",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "start": "nodemon app/server.js --exec babel-node --plugins transform-async-to-generator",
    "serve": "node -r babel-polyfill build/server.js",
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel app/ -d build --plugins transform-async-to-generator",
    "build-client": "browserify app/client.js -t babelify > build/client.js",
    "watch": "watchify app/client.js -t babelify -o build/client.js"
  }
}
